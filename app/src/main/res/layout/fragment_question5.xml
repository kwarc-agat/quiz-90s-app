<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentQuestion5">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="@dimen/size_num_icon"
            android:layout_height="@dimen/size_num_icon"
            android:layout_marginStart="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/page_number"
            android:gravity="center"
            android:text="@string/val_page_5"
            android:textColor="@color/burgund"
            android:textSize="@dimen/font_header"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/id_tv_q5"
            android:layout_width="284dp"
            android:layout_height="123dp"
            android:layout_marginTop="124dp"
            android:background="@drawable/border_green"
            android:gravity="center"
            android:scrollHorizontally="false"
            android:text="@string/question5"
            android:textColor="@color/burgund"
            android:textSize="@dimen/font_question"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/id_img_next5"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginStart="331dp"
            android:layout_marginLeft="331dp"
            android:layout_marginTop="156dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="100dp"
            android:src="@drawable/next1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/id_img_5_c" />

        <ImageView
            android:id="@+id/id_img_5_a"
            android:layout_width="157dp"
            android:layout_height="144dp"
            android:layout_marginStart="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginTop="@dimen/btn_margin_vert"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="21dp"
            android:background="@drawable/border_green"
            android:src="@drawable/kappa"
            app:layout_constraintBottom_toTopOf="@+id/id_img_5_b"
            app:layout_constraintEnd_toStartOf="@+id/id_img_5_c"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/id_tv_q5" />

        <ImageView
            android:id="@+id/id_img_5_b"
            android:layout_width="198dp"
            android:layout_height="118dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/border_green"
            android:src="@drawable/new_balance"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/id_img_5_a" />

        <ImageView
            android:id="@+id/id_img_5_c"
            android:layout_width="175dp"
            android:layout_height="142dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp"
            android:layout_marginBottom="21dp"
            android:background="@drawable/border_green"
            android:src="@drawable/quechua"
            app:layout_constraintBottom_toTopOf="@+id/id_img_5_b"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/id_img_5_a"
            app:layout_constraintTop_toBottomOf="@+id/id_tv_q5" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>